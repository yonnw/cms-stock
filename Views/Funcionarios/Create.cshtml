@model cms_stock.Models.Dominio.Entidades.Funcionario

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_LayoutQuixlab.cshtml";
}

<div class="content-body">
    <div class="card">
        <div class="card-body">
            <h4>Funcionário</h4>
            <hr />
            <div class="row">
                <div class="col-md-4">
                    <form asp-action="Create">
                        @Html.Partial("Form")
                    </form>
                </div>
            </div>
            <div>
                <a asp-action="Index">Voltar</a>
            </div>

            @section Scripts {
                @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
            }

        </div>
    </div>
</div>