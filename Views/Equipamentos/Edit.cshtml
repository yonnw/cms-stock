@model cms_stock.Models.Dominio.Entidades.Equipamento

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-body">
    <div class="card">
        <div class="card-body">
            <h1>Editar</h1>
            <h4>Equipamento</h4>
            <hr />
            <div class="row">
                <div class="col-md-4">
                    <form asp-action="Edit">
                        @Html.Partial("Form")
                    </form>
                </div>
            </div>
            <div>
                <a asp-action="Index">Voltar</a>
            </div>

            @section Scripts {
                @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
            }

        </div>
    </div>
</div>